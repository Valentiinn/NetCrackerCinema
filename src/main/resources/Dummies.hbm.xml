<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.netcracker.cinema.domains.Dummy" table="dummies">
        <id name="id" type="java.lang.Long" column="DUMMY_ID" />
        <property name="name" type="string" column="DUMMY_NAME" />
    </class>
</hibernate-mapping>